<?xml version="1.0" encoding="UTF-8"?>
<launch>
    <include file="$(find gazebo_ros)/launch/empty_world.launch">
        <arg name="world_name" value="$(find drone_openai)/worlds/actor.world"/>
        <arg name="use_sim_time" value="true"/>
        <arg name="verbose" value="true"/>
    </include>

    <param name="robot_description" textfile="$(find sjtu_drone)/models/sjtu_drone/sjtu_drone.urdf" />
    <node name="spawn_model" pkg="sjtu_drone" type="spawn_model" respawn="false" />

    <rosparam command="load" file="$(find drone_openai)/config/qlearn_params.yaml" />
    <!-- <node name="training" pkg="drone_openai"   type="start_training.py" output="screen"/> -->
    <!-- <node name="detection" pkg="drone_openai"  type="detect.py" output="screen"/> -->
</launch>